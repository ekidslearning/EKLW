<body class="main">
	<!--wrapper starts-->
    <div class="wrapper">
        <!--header starts-->
        <!--header ends-->
        <!--main starts-->
        <div id="main">
            <section class="fullwidth-background turquoise-plain-bg">
                        <!--container starts-->
                        <div class="container"> 
                        	<h2 class="dt-sc-hr-white-title">Parent's Profile</h2> 
                           
                            <table>
                                @foreach (var u in Model)
                                {
                                    if (u.U_Id == int.Parse(Session["Uid"].ToString()))
                                    {

                                        @*<tr>
                                            <td>
                                                <h2>@u.U_Id</h2>
                                            </td>
                                        </tr>*@
                                        <tr>
                                            <td>
                                                <h2>@u.UserName</h2>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h2>@u.Password</h2>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h1>Update Profile</h1>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <h3><a href="/Services/Update/@u.U_Id">Click to Update</a></h3>
                                            </td>
                                        </tr>


                                    }
                                }



                            </table>

                        </div>
                        <!--container ends-->
                    </section>
        </div>
        <!--main ends-->
        
        <!--footer starts-->
        <!--footer ends-->
        
